    "babel-preset-es2015": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "del": "^2.2.2",
    "expose-loader": "^0.7.3",
    "gulp": "^3.9.1",
    "gulp-html-replace": "^1.6.2",
    "gulp-inject": "^4.2.0",
    "path": "^0.12.7",
    "run-sequence": "^1.2.2",
    "systemjs": "^0.20.12",
    "systemjs-plugin-babel": "0.0.21",
